<h1>About Us</h1><hr>

<p>We are providing fresh vegetables since 1980's</p>

<%= form_with model: @article do |form| %>
  <div>
    <%= form.label :first_Name %><br>
    <%= form.text_field :first_Name %>
  </div>
  <div>
    <%= form.label :last_name %><br>
    <%= form.text_field :last_name %>
  </div>
  <div>
    <%= form.label :address %><br>
    <%= form.text_field :address %>
  </div>
  <div>
    <%= form.label :email_id %><br>
    <%= form.text_field :email_id %>
  </div>

  <div>
    <%= form.label :mobile_no %><br>
    <%= form.text_field :mobile_no%>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>